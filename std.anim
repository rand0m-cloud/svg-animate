translate(x,y, tree) {
  return <g :transform="translate(x,y)"> {{ tree }} </g>
}

rotate(amt , tree) {
  return <g :transform="rotate(amt)"> {{ tree }} </g>
}

center(svg) {
  box = bounding_box(svg)
  x = -1 * (box.width/2)
  y = -1 * (box.height/2)
  return <g :transform="translate(x,y)"> {{ svg }} </g>
}

screenCenter(svg) {
  return translate(screen_width/2, screen_height/2, center(svg))
}