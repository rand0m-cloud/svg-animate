test {
  a = arg 0
  fill_ = arg 1
  element = svg_new rect
  svg_assign element width a
  svg_assign element height a
  svg_assign element fill fill_
  return element
}

fadeIn {
  local_percent = arg 0
  tree = arg 1
  element = svg_new g
  svg_assign element opacity local_percent
  svg_append element tree
  return element
}

main {
  # line 1
  _0 = 200
  _1 = 1s
  _2 = "red"
  element0 = call test [_0, _2]
  play _1 element0  

  # line 2
  _3 = 10
  _4 = 1s
  _5 = "blue"
  element1 = call test [_3, _5]
  play _4 element1 
}
